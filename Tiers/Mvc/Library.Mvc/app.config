<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTcpBinding_BookService">
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_ItemProviderService">
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_AuthService">
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_IntegrationService">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://192.168.0.31:8098/services/provider"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ItemProviderService"
        contract="ItemProviderServices.ItemProviderService" name="NetTcpBinding_ItemProviderService" />
      <endpoint address="net.tcp://192.168.0.31:8095/services/authentication"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_AuthService"
        contract="AuthenticationServices.AuthService" name="NetTcpBinding_AuthService" />
      <endpoint address="net.tcp://192.168.0.31:8097/services/books"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_BookService"
        contract="BookServices.BookService" name="NetTcpBinding_BookService" />
      <endpoint address="net.tcp://192.168.0.31:8096/services/integration"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IntegrationService"
        contract="IntegrationServices.IntegrationService" name="NetTcpBinding_IntegrationService" />
    </client>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
</configuration>
