  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTcpBinding_BookService" maxReceivedMessageSize="2147483647">
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_ItemProviderService" maxReceivedMessageSize="2147483647">
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_AuthService" maxReceivedMessageSize="2147483647">
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_IntegrationService" maxReceivedMessageSize="2147483647">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://192.168.0.22:8098/services/provider"
