
@{
    ViewBag.Title = "Import File";
}



<div class="box-header with-border">
    <h3 class="box-title">Import Excel File</h3>
</div><!-- /.box-header -->


<div class="box-body">
    <form method="POST" enctype="multipart/form-data">
        <div class="form-group">
            @Html.ActionLink("Download Template","DownloadTemplateFile")
            <br/>
            <br/>
            <input type="file" class="btn btn-default btn-flat" name="filetoupload" id="exampleInputFile">
            @*<p class="help-block">Example block-level help text here.</p>*@
            <br/>
            <button type="submit" class="btn btn-primary btn-flat">Submit</button>
        </div>
    </form>
</div>
